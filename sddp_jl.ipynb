   "source": [
    "m = SDDPModel(\n",
    "                  sense = :Min,          \n",
    "                 stages = 5,             \n",
    "                 solver = ClpSolver(),   \n",
    "           risk_measure = Expectation(), \n",
    "        objective_bound = -2             \n",
    "                                        ) do sp, t\n",
    "    \n",
    "    # the state\n",
    "    @state(sp, 0 <= x <= 1, x0 == 0.5)\n",
    "    \n",
    "    # the control\n",
    "    @variable(sp, 0 <= u <= 0.5)\n",
    "    \n",
    "    # the noise (and dynamics)\n",
    "    @scenario(sp, omega = linspace(0, 0.3, 10), x == x0 + u - omega)\n",
    "    \n",
    "    # the objective\n",
    "    stageobjective!(sp, (sin(3 * t) - 1) * u)\n",
    "    \n",
    "end\n",
    "println(typeof(m))"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {
